(define (problem FreeCell-simple)
(:domain freecell)
(:objects
	diamond - suitsort
	N0 N1 N2 N3 - denomination 
	diamondA diamond0 - card
)

(:init

    ; (and 
	; 		(incell diamondA)
	; 		(home diamond0) 
	; 		(suit diamondA diamond)
	; 		(suit diamond0 diamond)
	; 		(value diamondA N1)
	; 		(value diamond0 N0)
	; 		(successor N1 N0)
	; 		(cellspace N2)
	; 		(successor N3 N2))

	; :effect (and
	; 	  (home ?card)
	; 	  (cellspace ?ncells)
	; 	  (not (incell ?card))
	; 	  (not (cellspace ?cells))
	; 	  (not (home ?homecard))

    (incell diamondA)
    (home diamond0)
    (suit diamondA diamond)
    (suit diamond0 diamond)
    (value diamondA N1)
    (value diamond0 N0)
	(successor N1 N0)
	(successor N2 N1)
	(successor N3 N2)
    (cellspace N2)

)
(:goal (home diamondA))
)


(define (problem FreeCell2-4)
(:domain freecell)
(:objects
	diamond club heart spade - suitsort
	N0 N1 N2 - denomination 
	diamondA club2 heartA diamond2 diamond0 heart0 - card
)
(:init
	(successor N1 N0)
	(successor N2 N1)
	(cellspace N2)
	(clear diamondA)
	(on diamondA heartA)
	(bottomcol heartA)
	(clear club2)
	(on club2 diamond2)
	(bottomcol diamond2)
	(colspace N0)
	(value diamondA N1)
	(suit diamondA diamond)
	(canstack diamondA club2)
	(value club2 N2)
	(suit club2 club)
	(value heartA N1)
	(suit heartA heart)
	(canstack heartA club2)
	(value diamond2 N2)
	(suit diamond2 diamond)
	(home diamond0)
	(value diamond0 N0)
	(suit diamond0 diamond)
	(home heart0)
	(value heart0 N0)
	(suit heart0 heart)
)
(:goal (home diamond2))
)

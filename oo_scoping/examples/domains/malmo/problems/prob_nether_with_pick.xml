        <AgentSection mode="Survival">
            <Name>The Q-Learner</Name>
            <AgentStart>
                <Placement x="0.5" y="209" z="0.5" pitch="10" yaw="180"/>
                <Inventory>
                    <InventoryItem type="diamond_pickaxe" slot="0"/>
                </Inventory>
            </AgentStart>
            <AgentHandlers>
                <!-- <ContinuousMovementCommands turnSpeedDegs="420"/> -->
                <DiscreteMovementCommands>
                    <ModifierList type="deny-list">
                        <command>attack</command>
                    </ModifierList>
                </DiscreteMovementCommands>
                <ContinuousMovementCommands>
                    <ModifierList type="allow-list">
                        <command>attack</command>
                    </ModifierList>
                </ContinuousMovementCommands>
                <ObservationFromRay/>
                <RewardForDamagingEntity>
                    <Mob type="Sheep" reward="1"/>
                    <Mob type="Pig" reward="-1"/>
                </RewardForDamagingEntity>
                <SimpleCraftCommands/>
                <ObservationFromNearbyEntities>
                    <Range name="entities" xrange="13" yrange="2" zrange="13" />
                </ObservationFromNearbyEntities>
                <ObservationFromFullStats/>{video_requirements}
            </AgentHandlers>
        </AgentSection>

